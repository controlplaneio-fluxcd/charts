{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "required": [
        "resources",
        "securityContext"
    ],
    "properties": {
        "affinity": {
            "default": {
                "nodeAffinity": {
                    "requiredDuringSchedulingIgnoredDuringExecution": {
                        "nodeSelectorTerms": [
                            {
                                "matchExpressions": [
                                    {
                                        "key": "kubernetes.io/os",
                                        "operator": "In",
                                        "values": [
                                            "linux"
                                        ]
                                    }
                                ]
                            }
                        ]
                    }
                }
            },
            "type": "object",
            "properties": {
                "nodeAffinity": {
                    "type": "object",
                    "properties": {
                        "requiredDuringSchedulingIgnoredDuringExecution": {
                            "type": "object",
                            "properties": {
                                "nodeSelectorTerms": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "matchExpressions": {
                                                "type": "array",
                                                "items": {
                                                    "type": "object",
                                                    "properties": {
                                                        "key": {
                                                            "type": "string"
                                                        },
                                                        "operator": {
                                                            "type": "string"
                                                        },
                                                        "values": {
                                                            "type": "array",
                                                            "items": {
                                                                "type": "string"
                                                            }
                                                        }
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "apiPriority": {
            "default": {
                "enabled": false,
                "level": "workload-high"
            },
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "level": {
                    "type": "string"
                }
            }
        },
        "commonAnnotations": {
            "type": "object"
        },
        "commonLabels": {
            "type": "object"
        },
        "extraArgs": {
            "type": "array",
            "uniqueItems": true,
            "items": {
                "type": "string"
            }
        },
        "extraEnvs": {
            "type": "array",
            "uniqueItems": true,
            "items": {
                "type": "object"
            }
        },
        "fullnameOverride": {
            "type": "string"
        },
        "httpRoute": {
            "default": {
                "annotations": {},
                "enabled": false,
                "hostnames": [],
                "parentRefs": []
            },
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "enabled": {
                    "default": false,
                    "type": "boolean"
                },
                "hostnames": {
                    "type": "array",
                    "uniqueItems": true,
                    "items": {
                        "type": "string"
                    }
                },
                "parentRefs": {
                    "type": "array",
                    "uniqueItems": true,
                    "items": {
                        "type": "object"
                    }
                }
            }
        },
        "image": {
            "type": "object",
            "required": [
                "repository"
            ],
            "properties": {
                "imagePullPolicy": {
                    "type": "string",
                    "enum": [
                        "IfNotPresent",
                        "Always",
                        "Never"
                    ]
                },
                "pullSecrets": {
                    "type": "array",
                    "uniqueItems": true,
                    "items": {
                        "type": "object"
                    }
                },
                "repository": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "ingress": {
            "default": {
                "annotations": {},
                "className": "",
                "enabled": false
            },
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "className": {
                    "type": "string"
                },
                "enabled": {
                    "default": false,
                    "type": "boolean"
                },
                "hosts": {
                    "type": "array",
                    "uniqueItems": true,
                    "items": {
                        "type": "object"
                    }
                },
                "tls": {
                    "type": "array",
                    "uniqueItems": true,
                    "items": {
                        "type": "object"
                    }
                }
            }
        },
        "livenessProbe": {
            "type": "object",
            "properties": {
                "tcpSocket": {
                    "type": "object",
                    "properties": {
                        "port": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "nameOverride": {
            "type": "string"
        },
        "networkPolicy": {
            "type": "object",
            "properties": {
                "create": {
                    "default": true,
                    "type": "boolean"
                },
                "ingress": {
                    "type": "object",
                    "properties": {
                        "namespaces": {
                            "type": "array",
                            "uniqueItems": true,
                            "items": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "nodeSelector": {
            "type": "object"
        },
        "podSecurityContext": {
            "default": {
                "fsGroup": 1337
            },
            "type": "object"
        },
        "priorityClassName": {
            "type": "string"
        },
        "rbac": {
            "type": "object",
            "properties": {
                "create": {
                    "type": "boolean"
                }
            }
        },
        "readinessProbe": {
            "type": "object",
            "properties": {
                "tcpSocket": {
                    "type": "object",
                    "properties": {
                        "port": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "readonly": {
            "default": false,
            "type": "boolean"
        },
        "resources": {
            "type": "object",
            "properties": {
                "limits": {
                    "type": "object",
                    "properties": {
                        "cpu": {
                            "type": "string"
                        },
                        "memory": {
                            "type": "string"
                        }
                    }
                },
                "requests": {
                    "default": {
                        "cpu": "10m",
                        "memory": "64Mi"
                    },
                    "type": "object",
                    "properties": {
                        "cpu": {
                            "type": "string"
                        },
                        "memory": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "securityContext": {
            "type": "object",
            "properties": {
                "allowPrivilegeEscalation": {
                    "default": false,
                    "type": "boolean"
                },
                "capabilities": {
                    "default": {
                        "drop": [
                            "ALL"
                        ]
                    },
                    "type": "object",
                    "properties": {
                        "drop": {
                            "type": "array",
                            "uniqueItems": true,
                            "items": {
                                "type": "string"
                            }
                        }
                    }
                },
                "readOnlyRootFilesystem": {
                    "default": true,
                    "type": "boolean"
                },
                "runAsNonRoot": {
                    "default": true,
                    "type": "boolean"
                },
                "seccompProfile": {
                    "default": {
                        "type": "RuntimeDefault"
                    },
                    "type": "object",
                    "properties": {
                        "type": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "serviceAccount": {
            "default": {
                "automount": true,
                "create": true,
                "name": ""
            },
            "type": "object",
            "properties": {
                "automount": {
                    "type": "boolean"
                },
                "create": {
                    "type": "boolean"
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "tolerations": {
            "type": "array",
            "uniqueItems": true,
            "items": {
                "type": "object"
            }
        },
        "transport": {
            "type": "string",
            "enum": [
                "sse",
                "http"
            ]
        }
    }
}
